import java.util.Scanner;

public class ThirdProblem {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		String country = sc.nextLine();
		String equipment = sc.nextLine();
		double mark = 0;
		if("Bulgaria".equals(country)) {
			switch(equipment) {
				case "ribbon":
					mark = 19;
					break;
				case "hoop":
					mark = 19.3;
					break;
				case "rope":
					mark = 18.9;
					break;
			}
		}
		
		if("Italy".equals(country)) {
			switch(equipment) {
				case "ribbon":
					mark = 18.7;
					break;
				case "hoop":
					mark = 18.8;
					break;
				case "rope":
					mark = 18.85;
					break;
			}
		}
		
		if("Russia".equals(country)) {
			switch(equipment) {
				case "ribbon":
					mark = 18.5;
					break;
				case "hoop":
					mark = 19.1;
					break;
				case "rope":
					mark = 18.6;
					break;
			}
		}
		
		double percentage = (1 - (mark / 20))*100;
		System.out.printf("The team of %s get %.3f on %s.%n", country, mark, equipment);
		System.out.printf("%.2f%%", percentage );

	}

}
