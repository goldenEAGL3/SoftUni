import java.util.Scanner;

public class SecProblem {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int countWon = 0;
		int countDrawn = 0;
		int countLost = 0;
		
		for(int i = 0; i < 3 ; i++) {
			String result = sc.nextLine();
			int result1 = Integer.parseInt("" + result.charAt(0));
			int result2 = Integer.parseInt("" + result.charAt(2));
			if(result1 > result2) {
				countWon++;
			}
			else if(result1 == result2) {
				countDrawn++;
			}
			else {
				countLost++;
			}
		}
		System.out.printf("Team won %d games.%n"
				+ "Team lost %d games.%n"
				+ "Drawn games: %d", 
				countWon,
				countLost,
				countDrawn);
	}

}
